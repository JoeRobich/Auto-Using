{
    "version": "2.0.0",
    "tasks": [
        {
			"type": "npm",
			"script": "watch",
			"problemMatcher": "$tsc-watch",
			"isBackground": true,
			"presentation": {
				"reveal": "never"
			},
			"group": {
				"kind": "build",
				"isDefault": true
			}
		},
		{
			"type": "npm",
			"script": "compile",
			"problemMatcher": "$tsc-watch",
			"isBackground": true,
			"presentation": {
				"reveal": "never"
			},
			"group": {
				"kind": "build",
				"isDefault": true
			}
		},
		{
			"label": "build",
			"command": "dotnet",
			"type": "process",
			"args": [
				"build",
				"${workspaceFolder}/AutoUsingCs/AutoUsing/AutoUsing.csproj"
			],
			"problemMatcher": "$msCompile"
		},
		{
			"label": "buildTest",
			"command": "dotnet",
			"type": "process",
			"args": [
				"build",
				"${workspaceFolder}/AutoUsingCs/TestProg/TestProg.csproj"
			],
			"problemMatcher": []
		},
		{
			"label": "buildServerClient",
			"dependsOn":[
				"build",
				"npm: build",
				
			]
		}
    ]
}