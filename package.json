{
	"name": "auto-using",
	"displayName": "Auto-Using for C#",
	"icon": "icon.png",
	"version": "0.7.4",
	"publisher": "Fudge",
	"description": "Provides intellisense for and imports references from all available sources.",
	"repository": {
		"type": "git",
		"url": "https://github.com/natanfudge/Auto-Using"
	},
	"bugs": {
		"url": "https://github.com/natanfudge/Auto-Using/issues"
	},
	"files": [
		"out/**",
		"!out/test/**"
	],
	"license": "SEE LICENSE IN LICENSE",
	"keywords": [
		"C#",
		"csharp",
		"import",
		"require",
		"using",
		"auto-using",
		"auto-import"
	],
	"engines": {
		"vscode": "^1.32.0"
	},
	"categories": [
		"Other",
		"Programming Languages"
	],
	"contributes": {
		"commands": [
			{
				"command": "extension.wipeCommon",
				"title": "Wipe Memory Of Commonly Used Completions"
			}
		],
		"keybindings": [
			{
				"command": "extension.wipeCommon",
				"key": "Ctrl+Shift+W"
			}
		]
	},
	"activationEvents": [
		"onLanguage:csharp"
	],
	"main": "./out/extension.js",
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"compileInc" :"tsc -p ./ --incremental",
		"watch": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "npm run compile && node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"typescript": "^3.3.1",
		"vscode": "^1.1.28",
		"tslint": "^5.12.1",
		"@types/node": "^10.12.21",
		"@types/mocha": "^5.2.5",
		"colors": "^1.3.3"
	},
	"extensionDependencies": [
		"ms-vscode.csharp"
	],
	"dependencies": {
		"find-parent-dir": "^0.3.0",
		"find-up-glob": "^1.0.0",
		"glob": "^7.1.3",
		"ts-node": "^7.0.1",
		"vscode-jsonrpc": "^4.0.0",
		"vscode-languageclient": "^5.2.1"
	}
}
